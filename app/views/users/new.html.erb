<h1>Users#new</h1>
<h2>Sign Up</h2>
<%= form_for @user do |f| %>
	<%= f.label :username %>
	<%= f.search_field :username %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.label "Retype password" %>
	<%= f.password_field :repassword %>

	<%= f.submit "Sign Up" %>
<% end %>
<%= flash[:danger] %>