<h1>Sessions#new</h1>


<%= form_for(:session, url: login_path) do |f| %>
	<%= f.label :username %>
	<%= f.search_field :username %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.submit "Log in" %>
<% end %>

<% if  flash[:danger] %>
	<div id="flash-danger">
		<%= flash[:danger] %>
	</div>
<% end %>